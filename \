#include<stdio.h>
int main(void){
    int a,b,sum;
    while(EOF != scanf("%d %d",&a,&b)){
        if(a !=0 && b!= 0){
            sum = a+b;
            printf("%d\n",sum);
        }else{
            break;
        }
    }
}
